{
  "name": "peynman/larapress-payamgostar",
  "description": "Sync larapress users with payamgostar crm.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Peyman Abdi",
      "email": "peyman.abdi.work@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Larapress\\PayamGostar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapress\\PayamGostar\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapress\\PayamGostar\\Providers\\PackageServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7|^8",
    "laravel/framework": "^8|^9|^10"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.5",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "orchestra/testbench": "^8",
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*"
  }
}
